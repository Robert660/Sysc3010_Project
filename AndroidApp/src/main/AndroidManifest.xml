<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="robert.sysc3010">

    <uses-permission android:name="android.permission.INTERNET" />

    <user-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <user-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NotifyClass"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="notify_filter" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".GPS" />
        <activity android:name=".test" />
        <activity android:name=".AddContacts" />
        <activity android:name=".End" />
        <activity android:name=".HiddenChangeBT"></activity>
    </application>

</manifest>